= render 'shared/breadcrumbs', mode: :index, depth: :test_case, cs_class: @cs_class, problem: @problem

- @lines.each do |test_case, hash|
  %h2= "Input #{test_case.id}:"
  %p
  %pre
    - hash[:input].each do |line|
      - if line == '...'
        = link_to line, test_case.input.url
      - else
        = line
  %p
  %h2= "Output #{test_case.id}:"
  %p
  %pre
    - hash[:output].each do |line|
      - if line == '...'
        = link_to line, test_case.output.url
      - else
        = line
